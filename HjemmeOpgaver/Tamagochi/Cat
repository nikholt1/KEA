package Modules;
package Modules;

import java.util.Arrays;

public class Cat extends Tamagochi {
    private boolean isDead;
    //
//    public Cat(String name, int age, String mood, int energy, boolean isDead) {
//        super(name, age, mood, energy);
//
//    }
    public Cat(String name) {
        super(name);

    }

    public void makeCat(Tamagochi a) {
        makeTamagotchi(a);
    }
    public boolean checkDead(Cat a) {
        if (readTamagotchis(a).contains("false")) {
            return false;
        } else {
            return true;
        }
    }


    @Override
    public void makeSound() {
        System.out.println("Mi ow!");
    }

    @Override
    public String toString() {
        return getName() + " " + getAge() + " " + getMood() + " " + getEnergy() + " " + getIsDead();
    }


}
