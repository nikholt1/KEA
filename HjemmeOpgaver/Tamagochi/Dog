package Modules;

public class Dog extends Tamagochi{
    public boolean isDead;


    public Dog(String name) {
        super(name);

    }

    @Override
    public void makeSound(){
        System.out.println("Wuph!");
    }

    @Override
    public String toString() {
        return getName() + " " + getAge() + " " + getMood() + " " + getEnergy() + " " + isDead;
    }

}
